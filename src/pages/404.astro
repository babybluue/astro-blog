---
import AvatarImage from '@components/basic/avatar-image.astro'
import NavLink from '@components/basic/nav-link.astro'
import ThemeButton from '@components/basic/theme-button.astro'
import BaseLayout from '@layouts/base-layout.astro'

const links = [
  { url: '/', title: '首页' },
  { url: '/notes', title: '笔记' },
  { url: '/tags', title: '标签' },
  { url: '/archive', title: '归档' },
]

const title = '404 Not Found'
const description = '找不到页面-didmax'
---

<BaseLayout title={title} description={description}>
  <main>
    <section class="flex flex-col items-center justify-center">
      <AvatarImage />
    </section>
    <section class="my-10 flex items-center justify-center gap-10 text-primary">
      <NavLink />
    </section>
    <section class="flex justify-center py-5 text-4xl">404</section>
  </main>
</BaseLayout>
